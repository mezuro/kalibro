---
client: false # 'true' for consuming a remote Kalibro Service; 'false' if Kalibro Service is installed on this machine

clientSettings:
    serviceAddress: "http://localhost:8080/KalibroService/" # Address of the remote KalibroService

serverSettings:
    loadDirectory: ~/.kalibro/projects # Projects will be loaded in this directory before analysis
    databaseSettings:
        databaseType: MYSQL # Possibilities: APACHE_DERBY MYSQL
        jdbcUrl: "jdbc:mysql://localhost:3306/kalibro"
        username: "kalibro"
        password: "kalibro"
